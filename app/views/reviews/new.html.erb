<h1>Review</h1>
<%= form_with model: @review, url: "/books/#{params[:book_id]}/reviews" do |form| %>
  <div>
    <%= form.label :rating %>
    <%= form.text_field :rating %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end%>