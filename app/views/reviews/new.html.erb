<h1>Review</h1>
<%= form_with model: @review do |form| %>
  <div>
    <%= form.label :rating %>
    <%= form.text_field :rating %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>
  <div>
    <%= form.label :user_id %>
    <%= form.text_field :user_id %>
  </div>
  <div>
    <%= form.label :book_id %>
    <%= form.text_field :book_id %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end%>
